{
  "_id": "{{@ system @}}",
  "authentication": "basic",
  "operations": {
    "products-list": {
      "id_expression": "{{ id }}",
      "method": "GET",
      "url": "v3/products"
    }
  },
  "password": "$SECRET(api_key)",
  "type": "system:rest",
  "url_pattern": "{{@ base_url @}}/%s",
  "username": "{{@ consumer_key @}}",
  "verify_ssl": true
}